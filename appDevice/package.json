{
  "name": "single-spa-portal-example-angular",
  "version": "1.0.0",
  "description": "Angular App that will be bundled into a library to be loaded on demand by a simpleSPA Portal App",
  "scripts": {
    "ng": "ng",
    "start": "node node_modules/.bin/webpack-dev-server --config webpack-dev.config.js  --port 6010",
    "build": "ng build",
    "watch:portal": "webpack-dev-server --port 9020",
    "build:portal": "webpack -p --progress --env.prod",
    "serve:portal": "webpack -p --progress --env.prod && node_modules/.bin/http-server ./release -p 9020"
  },
