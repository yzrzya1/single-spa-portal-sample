    "@angular/cli": "^6.1.3",
    "@angular/compiler-cli": "^6.1.2",
    "@types/node": "~8.9.4",
    "babel-core": "^6.26.3",
    "babel-loader": "^7.1.5",
    "babel-preset-env": "^1.7.0",
    "babel-preset-stage-0": "^6.24.1",
    "copy-webpack-plugin": "^4.5.2",
    "file-loader": "^1.1.11",
    "http-server": "^0.11.1",
    "ts-loader": "^4.4.2",
