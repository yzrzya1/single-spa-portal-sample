{
  "name": "single-spa-portal-example-angular",
  "version": "1.0.0",
  "description": "Angular App that will be bundled into a library to be loaded on demand by a simpleSPA Portal App",
  "scripts": {
    "ng": "ng",
    "start": "ng serve --port 6005",
    "build": "ng build",
    "watch:portal": "webpack-dev-server --port 9005",
    "build:portal": "webpack -p --progress --env.prod",
    "serve:portal": "webpack -p --progress --env.prod && node_modules/.bin/http-server ./release -p 9005"
  },
